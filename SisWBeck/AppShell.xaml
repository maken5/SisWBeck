<?xml version="1.0" encoding="UTF-8" ?>
<Shell
	x:Class="SisWBeck.AppShell"
	xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
	xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
	xmlns:views="clr-namespace:SisWBeck.Views"
	xmlns:local="clr-namespace:SisWBeck">

    <ShellContent
			Title="Início"
			Route="inicio"
			Icon="iconblank.png"
			ContentTemplate="{DataTemplate views:MainPage}" />

    <ShellContent
			Title="Configurações"
			Route="config"
			Icon="iconblank.png"
			ContentTemplate="{DataTemplate views:ConfiguracaoPage}" />
    
</Shell>
